#include<iostream>
using namespace std;

int main() {
	
	short year, ost;
	cin >> year;
	ost = year % 12;
	switch (ost) {
	case 4:
		cout << "Rat";
		break;
	case 5:
		cout << "Bull";
		break;
	case 6:
		cout << "Tiger";
		break;
	case 7:
		cout << "Rabbit or Cat";
		break;
	case 8:
		cout << "The Dragon";
		break;
	case 9:
		cout << "Snake";
		break;
	case 10:
		cout << "Horse";
		break;
	case 11:
		cout << "Goat or Sheep";
		break;
	case 0:
		cout << "A monkey";
		break;
	case 1:
		cout << "Petuh";
		break;
	case 2:
		cout << "Dog";
		break;
	case 3:
		cout << "Kaban";
		break;
	default:
		break;
	}

}
