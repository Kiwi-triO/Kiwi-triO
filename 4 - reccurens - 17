#include<iostream>

using namespace std;

int main() {
	setlocale(LC_ALL, "ru");
	double b1 = 4, b2 = 2, b, n;
	cout << "Введите n: "; cin >> n;
	cout << "b1 = 4\n" << "b2 = 2\n";
	for (int i = 3; i <= n; i++) {
		b = (b1 / i) + (n * n / b2);
		b1 = b2;
		b2 = b;
		cout << "b" << i << " = " << b << endl;
	}
}
