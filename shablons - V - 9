#include<iostream>

using namespace std;


template <typename T>
void input(T **arr, int n) { //передаю массив по указателю
	for (int i = 0; i < n; i++) {
		arr[i] = new int[n];
	}
	for (int i = 0; i < n; i++)
		for (int j = 0; j < n; j++)
			cin >> arr[i][j];

}

template <typename T>
void work(T** arr, int n) {
	T max = arr[0][0];
	int k = 0;
	for (int i = 0; i < n; i++)
		for (int j = 0; j < n; j++)
			if (max < arr[i][j])
				max = arr[i][j];

	for (int i = 0; i < n; i++)
		for (int j = 0; j < n; j++)
			if (max == arr[i][j])
				k++;
	cout << k << endl << endl;
	
}
template <typename T>
void output(T** arr, int n) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++)
			cout << arr[i][j] << " ";
		cout << endl;
	}

}

int main() {

	int n;
	cin >> n;
	int** arr = new int *[n];
	input(arr, n); work(arr, n); output(arr, n);
	

}
